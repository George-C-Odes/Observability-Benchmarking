{
  "name": "nextjs-dash",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@11.11.0",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001 -H 0.0.0.0",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run -c vitest.config.node.ts && vitest run -c vitest.config.dom.ts",
    "test:node": "vitest run -c vitest.config.node.ts",
    "test:dom": "vitest run -c vitest.config.dom.ts",
    "test:fast": "vitest run -c vitest.config.node.ts --maxWorkers=4 && vitest run -c vitest.config.dom.ts --maxWorkers=4",
    "test:watch": "vitest -c vitest.config.dom.ts",
    "test:coverage": "vitest run -c vitest.config.node.ts --coverage && vitest run -c vitest.config.dom.ts --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.8",
    "@mui/material": "7.3.8",
    "@mui/material-nextjs": "7.3.8",
    "date-fns": "4.1.0",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "25.3.2",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/xml2js": "0.4.14",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^28.1.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  }
}
